doctype html(lang='en')
head
    meta(charset='UTF-8') 
    title Shooting Game  
    link(rel='stylesheet', href='vendor/bootstrap/dist/css/bootstrap.min.css')  
    script(src='vendor/angular/angular.min.js') 
    script(src='vendor/socket.io-client/socket.io.js')
    link(rel='stylesheet', href='css/style.css')   
  body
    .row
      .navbar-form.navbar-left(role='search')
        div
          label(for='name') Name   
          input.username(type='text', placeholder='Search')  
        button#join.btn.btn-default Join  
        button.btn.btn-default Logout   
    #container.col-xs-10
      #player
    #users.col-xs-2
      h4 users
    script(src='vendor/jquery/dist/jquery.min.js')
    script(src='js/module.js')



